
<form action="/Login" method="POST">
    <div class="form-group d-flex flex-column justify-content-center w-25">
    <h2><%= title %></h2>
    <label for="email">Email Address</label>
    <input type="email" id="email" name="user_email" class="form-control" placeholder="Enter Email"/>

    <label for="password">Password</label>
    <input type="password" class="form-control" id="password" name="user_password" placeholder="Password">
    </div>



    <button type="submit" class="btn btn-primary">Submit</button>

</form>
